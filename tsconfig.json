{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "esModuleInterop": true,
    "target": "ES2021",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "dist",
    "sourceRoot": "src",
    "baseUrl": "./src-old",
    "paths": {
      "*": [
        "node_modules/*"
      ]
    },
    "incremental": true,
    "typeRoots": [
      "./src/types",
      "../node_modules/@types"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "exclude": [
    "dist/**/+"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
